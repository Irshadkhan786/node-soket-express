<!DOCTYPE html>
<html lang="en">
<head>
  <title>App || User List</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  {{> common_head}}
</head>
<body>
<!-- navbar -->
{{> header}}
<!-- navbar ends here -->
<div class="container-fluid">
    <div class="row">
        <div class="col-md-6">
            <h2>Registered User List</h2>
        </div>
        <div class="col-md-4"><a class="btn btn-success" href="/createUser">New User</a></div>
        <div class="col-md-2">
            
        </div>
    </div>
  
  
    <div class="row">
        
        <div class="col-md-8 col-offset-md-2">        
            <table class="table">
                <thead>
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Contact</th>
                    <th>User Role</th>
                    <th>Date of Joining</th>
                    <th>Action</th>
                </tr>
                </thead>
                <tbody>
                {{#each data}}
                <tr>
                    <td>{{name}}</td>
                    <td>{{email}}</td>
                    <td>{{contact}}</td>
                    <td>{{role}}</td>
                    <td></td>
                    <td>Edit || Delete</td>
                </tr>
                {{/each}}
                
                </tbody>
            </table>
            
             <ul class="pagination">
                 {{#each page_links}}
                 {{{link}}}
                 {{/each}}
             </ul> 
              
              
        </div>
    </div>
</div>
{{> footer  }}
</body>
</html>
